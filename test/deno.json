{
  "tasks": {
    "start": "deno run -A --unstable --watch main.tsx"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxFactory": "createElement",
    "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "test/jsx"
  },
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"]
    }
  },
  "imports": {
    "@/": "./",
    "test/jsx/jsx-runtime": "./jsx-runtime.ts",
    "islands/server": "../server.ts",
    "islands/client": "../client.ts",
    "islands/preact/jsx-runtime": "../src/preact/jsx-runtime.ts",
    "islands/react/jsx-runtime": "../src/react/jsx-runtime.ts",
    "std/": "https://deno.land/std@0.190.0/",
    "pipe": "https://deno.land/x/pipe@0.3.0/mod.ts",
    "clsx": "https://esm.sh/clsx@1.2.1",
    "rutt": "https://deno.land/x/rutt@0.1.0/mod.ts",
    "@twind/core": "https://esm.sh/@twind/core@1.1.3",
    "@twind/with-react/": "https://esm.sh/@twind/with-react@1.1.3/",
    "preact-render-to-string": "https://esm.sh/preact-render-to-string@6.2.0?target=es2022&deps=preact@10.16.0",
    "to-readable-stream": "https://esm.sh/to-readable-stream@4.0.0",
    "preact": "https://esm.sh/preact@10.16.0?target=es2022",
    "@headlessui/react": "https://esm.sh/@headlessui/react@1.7.15?alias=react:preact/compat,react-dom:preact/compat,@types/react:preact/compat&external=preact",
    "preact/compat": "https://esm.sh/*preact@10.16.0/compat?target=es2022&deps=preact/hooks",
    "preact/hooks": "https://esm.sh/*preact@10.16.0/hooks?target=es2022",
    "preact/debug": "https://esm.sh/preact@10.16.0/debug?target=es2022&deps=preact@10.16.0",
    "@preact/signals": "https://esm.sh/@preact/signals@1.1.3?target=es2022&deps=preact@10.16.0",
    "preact/jsx-runtime": "https://esm.sh/*preact@10.16.0/jsx-runtime.js?target=es2022",
    "preact/jsx-dev-runtime": "https://esm.sh/*preact@10.16.0/jsx-dev-runtime.js?target=es2022",
    "react": "https://esm.sh/react@18.2.0?dev",
    "react/": "https://esm.sh/react@18.2.0/",
    "react-dom": "https://esm.sh/react-dom@18.2.0?dev",
    "react-dom/": "https://esm.sh/react-dom@18.2.0/",
    "react-icons/ri/": "https://cdn.jsdelivr.net/gh/urielch/react-icons-ri@1.0.9/ico/",
    "maplibre-gl": "https://esm.sh/maplibre-gl@3.0.0-pre.5",
    "noop-tag": "https://esm.sh/noop-tag@2.0.0",
    "mrmime": "https://deno.land/x/mrmime@v1.0.1/mod.ts"
  }
}
